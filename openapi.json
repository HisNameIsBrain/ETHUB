{
  "openapi": "3.1.0",
  "info": {
    "title": "ethub-ollama-mcp",
    "description": "ethub-ollama-mcp MCP Server",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "/ETHUB"
    }
  ],
  "paths": {
    "/ping": {
      "post": {
        "summary": "Ping",
        "operationId": "tool_ping_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ping_form_model"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "title": "Response Tool Ping Post"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/ollama_generate": {
      "post": {
        "summary": "Ollama Generate",
        "operationId": "tool_ollama_generate_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ollama_generate_form_model"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "title": "Response Tool Ollama Generate Post"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                { "type": "string" },
                { "type": "integer" }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": ["loc", "msg", "type"],
        "title": "ValidationError"
      },
      "ollama_generate_form_model": {
        "properties": {
          "model": {
            "type": "string",
            "title": "Model",
            "description": ""
          },
          "prompt": {
            "type": "string",
            "title": "Prompt",
            "description": ""
          }
        },
        "type": "object",
        "required": ["model", "prompt"],
        "title": "ollama_generate_form_model"
      },
      "ping_form_model": {
        "properties": {
          "message": {
            "type": "string",
            "title": "Message",
            "description": ""
          }
        },
        "type": "object",
        "title": "ping_form_model"
      }
    }
  }
}
